{
    "name": "outboardphp/basic-app-skeleton",
    "description": "A basic skeleton app using the Outboard framework.",
    "type": "project",
    "keywords": ["php", "outboard", "skeleton", "app"],
    "license": "Unlicense",
    "homepage": "https://github.com/outboardphp/basic-app-skeleton",
    "authors": [
        {
            "name": "Garrett Whitehorn",
            "homepage": "http://garrettw.net/",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "outboardphp/outboard": "*",
        "outboardphp/di": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/framework",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../packages/dic",
            "options": {
                "symlink": true
            }
        }
    ],
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true,
      "minimum-stability": "dev",
      "prefer-stable": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}

